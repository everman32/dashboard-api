{
  "name": "dashboard-api",
  "version": "1.0.0",
  "description": "Application for working with account data (authorization, registration)",
  "main": "dist/main.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/main.js",
    "dev": "tsx watch src/main.ts",
    "prisma:gen": "npm exec prisma generate",
    "test": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.ts --coverage",
    "lint": "eslint",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --check \"src/**/*\" \"e2e/**/*\"",
    "format:fix": "prettier --write \"src/**/*\" \"e2e/**/*\"",
    "prepare": "husky install"
  },
  "keywords": [
    "nodejs",
    "authorization",
    "registration",
    "jwt"
  ],
  "author": "Victor Borodyn",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.2.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "reflect-metadata": "^0.2.2",
    "tslog": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.8",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/supertest": "^2.0.12",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-sonarjs": "^3.0.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "prettier": "^3.4.2",
    "prisma": "^4.2.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0"
  }
}
